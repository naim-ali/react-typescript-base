  "description": "Aurign react client application",
  "main": "index.tsx",
  "scripts": {
    "start": "gulp serve --webpackDevServer",
    "build": "gulp build",
    "build:prod": "gulp build --env=prod",
    "lint-ts": "eslint src/app/**/*.{ts,tsx} --fix --cache",
